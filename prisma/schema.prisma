datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model competition {
  id             Int      @id @default(autoincrement())
  exhibitionName String   @db.VarChar(255)
  date           DateTime
  prizePool      Int
  difficulty     String   @db.VarChar(255)
  location       String
  capacity       Int
  img            String
}
